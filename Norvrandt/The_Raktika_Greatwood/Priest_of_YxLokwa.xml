<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Priest_of_YxLokwa "1">
]>
<Profile>
	<Name>Priest of Yx'Lokwa</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Priest of Yx'Lokwa..." />
		<If condition="not HasAtLeast(27587,50)">
			<!--Robber Ball-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':27587,'Amount':50,'Enabled':true,'Type':'Exchange'}]" />
		</If>
		<If condition="not IsOnSubMap(3166)">
			<LisbethTravel Area="The Rak'tika Greatwood" XYZ="-457.0445, -0.3, 401.5692" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="Condition.IsTimeBetween(9,10)">
			<Log Message="Waiting for window to open..." />
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Robber Ball" condition="not HasAtLeast(28071,&Priest_of_YxLokwa;) and HasAtLeast(27587,1) and Condition.IsTimeBetween(10,12)" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" CordialType="Auto" Sit="True" SitRate="1" Mooch="3" Patience="Patience2" MinimumGPPatience="650">
			<PatientTugs>
				<PatienceTug moochLevel="0" TugType="Light" />
				<PatienceTug moochLevel="1" TugType="Medium" />
				<PatienceTug moochLevel="2" TugType="Medium" />
			</PatientTugs>
			<FishSpots>
				<FishSpot XYZ="-457.0445, -0.3, 401.5692" Heading="5.239766" />
				<FishSpot XYZ="-488.3843, -0.3000001, 399.7425" Heading="4.875339" />
				<FishSpot XYZ="-482.7643, -0.3, 435.7073" Heading="6.037733" />
			</FishSpots>
			<Keepers>
				<Keeper name="Priest of Yx'Lokwa" />
			</Keepers>
			<SurfaceSlaps />
		</ExFish>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>