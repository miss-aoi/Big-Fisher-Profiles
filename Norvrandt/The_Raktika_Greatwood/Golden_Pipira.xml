<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Golden_Pipira "1">
]>
<Profile>
	<Name>Golden Pipira</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Golden Pipira..." />
		<If condition="not HasAtLeast(27587,50)">
			<!--Robber Ball-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':27587,'Amount':50,'Enabled':true,'Type':'Exchange'}]" />
		</If>
		<If condition="not IsOnSubMap(3183)">
			<LisbethTravel Area="The Rak'tika Greatwood" XYZ="197.1814, -27.50148, 725.8964" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="IsTimeBetween(5,6)">
			<Log Message="Waiting for window to open..." />
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Robber Ball" condition="not HasAtLeast(33324,&Golden_Pipira;) and HasAtLeast(27587,1) and IsTimeBetween(6,7)" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" CordialType="Auto" Sit="True" Chum="True">
			<FishSpots>
				<FishSpot XYZ="201.3549, -29.04337, 737.2993" Heading="0.9375334" />
				<FishSpot XYZ="221.9521, -25.88206, 725.4747" Heading="0.4827638" />
				<FishSpot XYZ="208.3455, -28.80793, 734.0103" Heading="0.8276381" />
			</FishSpots>
			<Keepers>
				<Keeper name="Golden Pipira" />
			</Keepers>
			<SurfaceSlaps />
		</ExFish>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>