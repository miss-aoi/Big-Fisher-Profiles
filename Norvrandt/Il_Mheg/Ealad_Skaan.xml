<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Ealad_Skaan "1">
]>
<Profile>
	<Name>Ealad Skaan</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Ealad Skaan..." />
		<If condition="not HasAtLeast(27589,50)">
			<!--Baitbugs-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':27589,'Amount':50,'Enabled':true,'Type':'Exchange'}]" />
		</If>
		<If condition="not IsOnSubMap(3144)">
			<LisbethTravel Area="Il Mheg" XYZ="-294.1939, 0.9144536, 182.5432" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="Condition.IsTimeBetween(22.5,23.5)">
			<Log Message="Waiting for window to open..." />
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Baitbugs" condition="not HasAtLeast(33242,&Ealad_Skaan;) and HasAtLeast(27589,1) and Condition.IsTimeBetween(23.5,24) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(816, 'Clear Skies') and ExBuddy.Helpers.SkywatcherPlugin.PredictWeatherInZone(816, TimeSpan.FromHours(-8), 'Thunderstorms')"
				MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" CordialType="Auto" Sit="True" SitRate="1" Chum="True">
			<FishSpots>
				<FishSpot XYZ="-291.337, -0.3474985, 174.3676" Heading="2.237679" />
				<FishSpot XYZ="-303.5754, -0.3267224, 149.0237" Heading="2.56231" />
				<FishSpot XYZ="-278.1725, 0.2184548, 193.5368" Heading="2.038707" />
			</FishSpots>
			<Keepers>
				<Keeper name="Ealad Skaan" />
			</Keepers>
			<SurfaceSlaps />
		</ExFish>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>