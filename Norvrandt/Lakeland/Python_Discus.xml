			<FishSpots>
				<FishSpot XYZ="-27.86252, -0.4972355, 636.3398" Heading="1.897425" />
				<FishSpot XYZ="-32.49625, -0.4892691, 620.8665" Heading="3.338368" />
				<FishSpot XYZ="-32.66836, -0.4741216, 645.6116" Heading="4.77931" />
			</FishSpots>
			<Keepers>
				<Keeper name="Python Discus" />
			</Keepers>
