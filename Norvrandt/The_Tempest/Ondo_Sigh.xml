<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Ondo_Sigh "1">
]>
<Profile>
	<Name>Ondo Sigh</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Ondo Sigh..." />
		<If condition="not HasAtLeast(27590,50)">
			<!--Squid Strip-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':27590,'Amount':50,'Enabled':true,'Type':'Exchange'}]" />
		</If>
		<If condition="not IsOnSubMap(3196)">
			<LisbethTravel Area="The Tempest" XYZ="-428.7982, 393.7201, -670.8645" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="IsTimeBetween(11,12)">
			<Log Message="Waiting for window to open..." />
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Squid Strip" condition="not HasAtLeast(28930,&Ondo_Sigh;) and HasAtLeast(27590,1) and IsTimeBetween(12,14) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(818, 'Clear Skies', 'Fair Skies')" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" CordialType="HiCordial" Chum="True">
			<FishSpots>
				<FishSpot XYZ="-428.7982, 393.7201, -670.8645" Heading="4.822519" />
				<FishSpot XYZ="-453.8247, 393.7201, -675.4504" Heading="0.3467791" />
				<FishSpot XYZ="-462.4988, 393.72, -650.2955" Heading="5.239284" />
			</FishSpots>
			<Keepers>
				<Keeper name="Ondo Sigh" />
			</Keepers>
			<SurfaceSlaps />
		</ExFish>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>