<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY The_Ondotaker "1">
]>
<Profile>
	<Name>The Ondotaker</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: The Ondotaker..." />
		<If condition="not HasAtLeast(27590,50)">
			<!--Squid Strip-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':27590,'Amount':50,'Enabled':true,'Type':'Exchange'}]" />
		</If>
		<If condition="not IsOnSubMap(3199)">
			<LisbethTravel Area="The Tempest" XYZ="785.0533, 448.4622, -719.5323" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="IsTimeBetween(7,8)">
			<Log Message="Waiting for window to open..." />
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Squid Strip" condition="not HasAtLeast(32054,&The_Ondotaker;) and HasAtLeast(27590,1) and IsTimeBetween(8,10) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(818, 'Clouds') and ExBuddy.Helpers.SkywatcherPlugin.PredictWeatherInZone(818, TimeSpan.FromHours(-8), 'Clear Skies')" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" CordialType="Auto" Sit="True" SitRate="1" Chum="True">
			<FishSpots>
				<FishSpot XYZ="770.9969, 446.4425, -721.9981" Heading="4.427551" />
				<FishSpot XYZ="777.5337, 448.4783, -732.9607" Heading="3.981445" />
				<FishSpot XYZ="785.8091, 448.0647, -744.4491" Heading="3.619115" />
			</FishSpots>
			<Keepers>
				<Keeper name="The Ondotaker" />
			</Keepers>
			<SurfaceSlaps />
		</ExFish>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>