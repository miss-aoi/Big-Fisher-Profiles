<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Bobgoblin_Bass "1">
]>
<Profile>
	<Name>Bobgoblin Bass</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Bobgoblin Bass..." />
		<If condition="not HasAtLeast(12706,3)">
			<!--Goblin Jig-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':12706,'Amount':3,'Enabled':true,'Type':'Goldsmith'}]" />
		</If>
		<If condition="not IsOnSubMap(2039)">
			<LisbethTravel Area="The Dravanian Hinterlands" XYZ="-60.88793, 53.95971, 121.7971" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="IsTimeBetween(1,2)">
			<Log Message="Waiting for window to open..." />
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Goblin Jig" condition="not HasAtLeast(16754,&Bobgoblin_Bass;) and HasAtLeast(12706,1) and IsTimeBetween(2,6) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(398, 'Showers') and ExBuddy.Helpers.SkywatcherPlugin.PredictWeatherInZone(398, TimeSpan.FromHours(-8), 'Rain')" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" CordialType="HiCordial" Mooch="2" Patience="Patience2" MinimumGPPatience="650">
			<PatientTugs>
				<PatienceTug moochLevel="0" TugType="Medium" />
				<PatienceTug moochLevel="1" TugType="Medium" />
			</PatientTugs>
			<FishSpots>
				<FishSpot XYZ="-55.23393, 53.8129, 119.0049" Heading="1.762815" />
				<FishSpot XYZ="-54.50538, 54.01475, 142.2572" Heading="0.7616963" />
				<FishSpot XYZ="-54.34693, 54.25716, 128.7334" Heading="2.401608" />
			</FishSpots>
			<Keepers>
				<Keeper name="Bobgoblin Bass" />
			</Keepers>
			<SurfaceSlaps />
		</ExFish>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>