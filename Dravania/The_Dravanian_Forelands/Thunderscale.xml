<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Thunderscale "1">
]>
<Profile>
	<Name>Thunderscale</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Thunderscale..." />
		<If condition="not HasAtLeast(12704,50)">
			<!--Stonefly Nymph-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':12704,'Amount':50,'Enabled':true,'Type':'Purchase'}]" />
		</If>
		<If condition="not IsOnSubMap(2029)">
			<LisbethTravel Area="The Dravanian Forelands" XYZ="458.238, 58.42916, -809.2572" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="IsTimeBetween(5,6)">
			<Log Message="Waiting for window to open..." />
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Stonefly Nymph" condition="not HasAtLeast(17582,&Thunderscale;) and HasAtLeast(12704,1) and IsTimeBetween(6,8) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(398, 'Thunder') and ExBuddy.Helpers.SkywatcherPlugin.PredictWeatherInZone(398, TimeSpan.FromHours(-8), 'Clouds', 'Dust Storms', 'Fog')" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" CordialType="HiCordial" Mooch="2" Patience="Patience2" MinimumGPPatience="650">
			<PatientTugs>
				<PatienceTug moochLevel="0" TugType="Light" />
				<PatienceTug moochLevel="1" TugType="Medium" />
			</PatientTugs>
			<FishSpots>
				<FishSpot XYZ="458.238, 58.42916, -809.2572" Heading="3.394347" />
				<FishSpot XYZ="480.1388, 58.42916, -845.8835" Heading="3.762961" />
				<FishSpot XYZ="428.7514, 58.42916, -846.7012" Heading="5.36937" />
			</FishSpots>
			<Keepers>
				<Keeper name="Thunderscale" />
			</Keepers>
			<SurfaceSlaps />
		</ExFish>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>