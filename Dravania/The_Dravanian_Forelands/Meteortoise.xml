			</PatientTugs>
			<FishSpots>
				<FishSpot XYZ="-471.8031, -10.14293, -475.2836" Heading="1.936656" />
				<FishSpot XYZ="-478.3157, -11.6859, -448.0603" Heading="3.788103" />
				<FishSpot XYZ="-455.5224, -12.17101, -456.36" Heading="3.277071" />
			</FishSpots>
			<Keepers>
				<Keeper name="Meteortoise" />
			</Keepers>
			<SurfaceSlaps />
