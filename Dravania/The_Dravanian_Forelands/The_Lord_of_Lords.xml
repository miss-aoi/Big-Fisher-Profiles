<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY The_Lord_of_Lords "1">
]>
<Profile>
	<Name>The Lord of Lords</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: The Lord of Lords..." />
		<If condition="not HasAtLeast(28634,3)">
			<!--Metal Spinner-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':28634,'Amount':3,'Enabled':true,'Type':'Purchase'}]" />
		</If>
		<If condition="not IsOnSubMap(2030)">
			<LisbethTravel Area="The Dravanian Forelands" XYZ="-737.6535, 4.907196, -819.5856" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="IsTimeBetween(7,8)">
			<Log Message="Waiting for window to open..." />
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Metal Spinner" condition="not HasAtLeast(17584,&The_Lord_of_Lords;) and HasAtLeast(28634,1) and IsTimeBetween(8,16) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(398, 'Clear Skies', 'Fair Skies') and ExBuddy.Helpers.SkywatcherPlugin.PredictWeatherInZone(398, TimeSpan.FromHours(-8), 'Clouds', 'Dust Storms', 'Fog')" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" CordialType="HiCordial" Sit="True" Mooch="2" Patience="Patience2" MinimumGPPatience="650">
			<PatientTugs>
				<PatienceTug moochLevel="0" TugType="Light" />
				<PatienceTug moochLevel="1" TugType="Medium" />
			</PatientTugs>
			<FishSpots>
				<FishSpot XYZ="-741.1226, 5.558385, -824.1998" Heading="4.014312" />
				<FishSpot XYZ="-730.9006, 5.529306, -837.8148" Heading="3.964034" />
				<FishSpot XYZ="-741.4175, 5.576777, -823.7872" Heading="5.040553" />
			</FishSpots>
			<Keepers>
				<Keeper name="The Lord of Lords" />
			</Keepers>
			<SurfaceSlaps />
		</ExFish>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>