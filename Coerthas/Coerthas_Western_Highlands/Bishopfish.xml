<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Bishopfish "1">
]>
<Profile>
	<Name>Bishopfish</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Bishopfish..." />
		<If condition="not HasAtLeast(12711,50)">
			<!--Brute Leech-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':12711,'Amount':50,'Enabled':true,'Type':'Exchange'}]" />
		</If>
		<If condition="not IsOnSubMap(2201)">
			<LisbethTravel Area="Coerthas Western Highlands" XYZ="694.4288, 131.6431, -803.657" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="Condition.IsTimeBetween(9,10)">
			<Log Message="Waiting for window to open..." />
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Brute Leech" condition="not HasAtLeast(17577,&Bishopfish;) and HasAtLeast(12711,1) and Condition.IsTimeBetween(10,14) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(397, 'Clear Skies')"
				MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" CordialType="Auto" Sit="True" SitRate="1" Chum="True">
			<FishSpots>
				<FishSpot XYZ="700.9606, 130.4927, -812.5688" Heading="2.43003" />
				<FishSpot XYZ="705.4149, 130.772, -807.4819" Heading="2.409086" />
				<FishSpot XYZ="697.2401, 130.4616, -814.8465" Heading="2.989224" />
			</FishSpots>
			<Keepers>
				<Keeper name="Bishopfish" />
			</Keepers>
			<SurfaceSlaps />
		</ExFish>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>