<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Bishopfish "1">
]>
<Profile>
	<Name>Bishopfish</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Bishopfish..." />
		<If condition="not HasAtLeast(12711,50)">
			<!--Brute Leech-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':12711,'Amount':50,'Enabled':true,'Type':'Exchange'}]" />
		</If>
		<If condition="not IsOnSubMap(2202)">
			<LisbethTravel Area="Coerthas Western Highlands" XYZ="-32.91364, 89.56251, -571.7196" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="IsTimeBetween(9,10)">
			<Log Message="Waiting for window to open..." />
                        <WaitTimer WaitTime="30" />
		</While>
		<While condition="not HasAtLeast(17577,&Bishopfish;) and HasAtLeast(12711,1) and IsTimeBetween(10,14) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(397, 'Clear Skies')">
			<ExFish Bait="Brute Leech" condition="not HasAtLeast(17577,&Bishopfish;) and HasAtLeast(12711,1) and IsTimeBetween(10,14) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(397, 'Clear Skies')" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" Cordial="Auto" Sit="True" Chum="True">
				<FishSpots>
					<FishSpot XYZ="-21.4197, 89.14197, -559.7135" Heading="4.573871" />
					<FishSpot XYZ="-32.44563, 89.25902, -571.203" Heading="6.274516" />
					<FishSpot XYZ="-48.33238, 89.17406, -557.7254" Heading="1.37154" />
				</FishSpots>
				<Keepers>
					<Keeper name="Bishopfish" />
				</Keepers>
				<SurfaceSlaps />
			</ExFish>
		</While>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>