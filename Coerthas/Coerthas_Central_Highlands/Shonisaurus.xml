<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Shonisaurus "1">
]>
<Profile>
	<Name>Shonisaurus</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Shonisaurus..." />
		<If condition="not HasAtLeast(2605,50)">
			<!--Hoverworm-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2605,'Amount':50,'Enabled':true,'Type':'Purchase'}]" />
		</If>
		<If condition="not IsOnSubMap(393)">
			<LisbethTravel Area="Coerthas Central Highlands" XYZ="580.0012, 302.5874, -163.7016" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="not HasAtLeast(8772,&Shonisaurus;) and HasAtLeast(2605,1) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(155, 'Blizzards') and ExBuddy.Helpers.SkywatcherPlugin.PredictWeatherInZone(155, TimeSpan.FromHours(-8), 'Blizzards')">
			<ExFish Bait="Hoverworm" condition="not HasAtLeast(8772,&Shonisaurus;) and HasAtLeast(2605,1) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(155, 'Blizzards') and ExBuddy.Helpers.SkywatcherPlugin.PredictWeatherInZone(155, TimeSpan.FromHours(-8), 'Blizzards')" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" Cordial="Auto" Sit="True" Mooch="3" Patience="Patience2" MinimumGPPatience="650">
				<PatientTugs>
					<PatienceTug moochLevel="0" TugType="Medium" />
					<PatienceTug moochLevel="1" TugType="Medium" />
					<PatienceTug moochLevel="2" TugType="Medium" />
				</PatientTugs>
				<FishSpots>
					<FishSpot XYZ="576.5197, 300.6741, -171.3599" Heading="3.279671" />
					<FishSpot XYZ="583.305, 300.7844, -148.4045" Heading="0.9653695" />
					<FishSpot XYZ="587.6274, 302.0664, -169.4482" Heading="2.506844" />
				</FishSpots>
				<Keepers>
					<Keeper name="Shonisaurus" />
				</Keepers>
				<SurfaceSlaps />
			</ExFish>
		</While>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>