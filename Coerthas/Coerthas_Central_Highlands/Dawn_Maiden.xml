<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Dawn_Maiden "1">
]>
<Profile>
	<Name>Dawn Maiden</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Dawn Maiden..." />
		<If condition="not HasAtLeast(2623,3)">
			<!--Wildfowl Fly-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2623,'Amount':3,'Enabled':true,'Type':'Purchase'}]" />
		</If>
		<If condition="not IsOnSubMap(380)">
			<LisbethTravel Area="Coerthas Central Highlands" XYZ="325.3246, 303.2775, -254.3248" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="IsTimeBetween(4,5)">
			<WaitTimer WaitTime="30" />
		</While>
		<While condition="not HasAtLeast(7944,&Dawn_Maiden;) and HasAtLeast(2623,1) and IsTimeBetween(5,7) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(155, 'Clear Skies', 'Fair Skies')">
			<ExFish Bait="Wildfowl Fly" and condition="not HasAtLeast(7944,&Dawn_Maiden;) and HasAtLeast(2623,1) and IsTimeBetween(5,7) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(155, 'Clear Skies', 'Fair Skies')" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" Cordial="Auto" Sit="True" Chum="True">
				<FishSpots>
					<FishSpot XYZ="290.7569, 301, -251.3285" Heading="0.2386003" />
					<FishSpot XYZ="309.8876, 301, -255.1656" Heading="5.388716" />
					<FishSpot XYZ="320.1285, 301, -246.6241" Heading="5.231636" />
				</FishSpots>
				<Keepers>
					<Keeper name="Dawn Maiden" />
				</Keepers>
				<SurfaceSlaps />
			</ExFish>
		</While>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>