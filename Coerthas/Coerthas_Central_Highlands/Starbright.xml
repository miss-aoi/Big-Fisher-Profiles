<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Starbright "1">
]>
<Profile>
	<Name>Starbright</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Starbright..." />
		<If condition="not HasAtLeast(2597,50)">
			<!--Syrphid Basket-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2597,'Amount':50,'Enabled':true,'Type':'Purchase'}]" />
		</If>
		<If condition="not IsOnSubMap(383)">
			<LisbethTravel Area="Coerthas Central Highlands" XYZ="-299.6851, 244.8674, 12.87033" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="IsTimeBetween(19,22)">
			<Log Message="Waiting for window to open..." />
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Syrphid Basket" condition="not HasAtLeast(7945,&Starbright;) and HasAtLeast(2597,1) and (IsTimeBetween(20,24) or IsTimeBetween(0,4)) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(155, 'Clear Skies', 'Fair Skies')" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" CordialType="HiCordial" Mooch="2" Patience="Patience2" MinimumGPPatience="650">
			<PatientTugs>
				<PatienceTug moochLevel="0" TugType="Light" />
				<PatienceTug moochLevel="1" TugType="Medium" />
			</PatientTugs>
			<FishSpots>
				<FishSpot XYZ="-269.9615, 248.1604, 10.56468" Heading="0.02401352" />
				<FishSpot XYZ="-292.2026, 242.4896, 17.62959" Heading="1.029324" />
				<FishSpot XYZ="-299.8128, 242.6337, 32.33716" Heading="1.896401" />
			</FishSpots>
			<Keepers>
				<Keeper name="Starbright" />
			</Keepers>
			<SurfaceSlaps />
		</ExFish>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>