<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Anomalocaris "1">
]>
<Profile>
	<Name>Anomalocaris</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Anomalocaris..." />
		<If condition="not HasAtLeast(2605,50)">
			<!--Hoverworm-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2605,'Amount':50,'Enabled':true,'Type':'Purchase'}]" />
		</If>
		<If condition="not IsOnSubMap(383)">
			<LisbethTravel Area="Coerthas Central Highlands" XYZ="-452.3188, 202.7171, -362.1353" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="Condition.IsTimeBetween(9,10)">
			<Log Message="Waiting for window to open..." />
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Hoverworm" condition="not HasAtLeast(7949,&Anomalocaris;) and HasAtLeast(2605,1) and Condition.IsTimeBetween(10,15) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(155, 'Clear Skies', 'Fair Skies')"
				MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" CordialType="Auto" Sit="True" SitRate="1"
				Mooch="2" Patience="Patience2" MinimumGPPatience="650">
			<PatientTugs>
				<PatienceTug moochLevel="0" TugType="Medium" />
				<PatienceTug moochLevel="1" TugType="Medium" />
			</PatientTugs>
			<FishSpots>
				<FishSpot XYZ="-433.0484, 200.7377, -358.037" Heading="1.987735" />
				<FishSpot XYZ="-448.98, 200.6491, -369.0937" Heading="2.616056" />
				<FishSpot XYZ="-476.9392, 200.8663, -388.2444" Heading="2.457772" />
			</FishSpots>
			<Keepers>
				<Keeper name="Anomalocaris" />
			</Keepers>
			<SurfaceSlaps />
		</ExFish>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>