<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Vip_Viper "1">
]>
<Profile>
	<Name>Vip Viper</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Vip Viper..." />
		<If condition="not HasAtLeast(2594,50)">
			<!--Butterworm-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2594,'Amount':50,'Enabled':true,'Type':'Purchase'}]" />
		</If>
		<If condition="not IsOnSubMap(78)">
			<LisbethTravel Area="South Shroud" XYZ="-179.9766, 10.14908, -139.1424" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="IsTimeBetween(17,18)">
			<WaitTimer WaitTime="30" />
		</While>
		<While condition="not HasAtLeast(7908,&Vip_Viper;) and HasAtLeast(2594,1) and IsTimeBetween(18,19)">
			<ExFish Bait="Butterworm" condition="not HasAtLeast(7908,&Vip_Viper;) and HasAtLeast(2594,1) and IsTimeBetween(18,19)" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" Cordial="Auto" Sit="True" Chum="True">
				<FishSpots>
					<FishSpot XYZ="-161.2432, 6.347555, -109.6849" Heading="0.8256314" />
					<FishSpot XYZ="-181.6259, 6.686643, -127.521" Heading="4.559937" />
					<FishSpot XYZ="-203.8454, 8.566397, -140.8594" Heading="4.538989" />
				</FishSpots>
				<Keepers>
					<Keeper name="Vip Viper" />
				</Keepers>
				<SurfaceSlaps />
			</ExFish>
		</While>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>