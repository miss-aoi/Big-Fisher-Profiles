<?xml version='1.0' encoding='utf8'?>
<!--
	Profile: Levinlight (7920)
	Authors: miss-aoi
-->
<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Levinlight "1">
]>
<Profile>
	<Name>Levinlight</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Levinlight..." />
		<If condition="not HasAtLeast(2597,50)">
			<!--Syrphid Basket-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2597,'Amount':50,'Enabled':true,'Type':'Purchase'}]" />
		</If>
		<If condition="not IsOnSubMap(70)">
			<LisbethTravel Area="Central Shroud" XYZ="-315.058, 21.97407, 147.0868" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="Condition.IsTimeBetween(17,18)">
			<Log Message="Waiting for window to open..." />
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Syrphid Basket" condition="not HasAtLeast(7920,&Levinlight;) and HasAtLeast(2597,1) and Condition.IsTimeBetween(18,23)"
				MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" CordialType="Auto" Sit="True" SitRate="1" Chum="True">
			<FishSpots>
				<FishSpot XYZ="-295.7599, 20.99179, 169.1394" Heading="5.806733" />
				<FishSpot XYZ="-303.1868, 21, 193.6298" Heading="2.94788" />
				<FishSpot XYZ="-311.5812, 20.99179, 154.1951" Heading="0.3403623" />
			</FishSpots>
			<Keepers>
				<Keeper name="Levinlight" />
			</Keepers>
			<SurfaceSlaps />
		</ExFish>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>