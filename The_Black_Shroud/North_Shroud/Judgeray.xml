					<FishSpot XYZ="-37.19979, -40.94998, 194.5685" Heading="5.929203" />
				</FishSpots>
				<Keepers>
					<Keeper name="Judgeray" />
				</Keepers>
				<SurfaceSlaps />
			</ExFish>
