<Profile>
	<Name>Judgeray</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Judgeray..." />
		<If condition="not HasAtLeast(2623,50)">
			<!--Wildfowl Fly-->
