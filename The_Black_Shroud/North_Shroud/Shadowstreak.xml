<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Shadowstreak "1">
]>
<Profile>
	<Name>Shadowstreak</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Shadowstreak..." />
		<If condition="not HasAtLeast(2624,3)">
			<!--Topwater Frog-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2624,'Amount':3,'Enabled':true,'Type':'Goldsmith'}]" />
		</If>
		<If condition="not IsOnSubMap(145)">
			<LisbethTravel Area="North Shroud" XYZ="-30.23347, -9.153938, -46.34664" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="Condition.IsTimeBetween(3.5,4.5)">
			<Log Message="Waiting for window to open..." />
                        <WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Topwater Frog" condition="not HasAtLeast(7947,&Shadowstreak;) and HasAtLeast(2624,1) and Condition.IsTimeBetween(4.5,10) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(154, 'Fog')" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" CordialType="Auto" Sit="True" SitRate="1" Chum="True">
			<FishSpots>
				<FishSpot XYZ="-26.52839, -10.12451, -52.77297" Heading="2.658334" />
				<FishSpot XYZ="-27.09962, -8.5765, -42.95875" Heading="6.019836" />
				<FishSpot XYZ="-28.64076, -12.07396, -63.1396" Heading="1.489659" />
			</FishSpots>
			<Keepers>
				<Keeper name="Shadowstreak" />
			</Keepers>
			<SurfaceSlaps />
		</ExFish>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>