<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Dark_Ambusher "1">
]>
<Profile>
	<Name>Dark Ambusher</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Dark Ambusher..." />
		<If condition="not HasAtLeast(2586,50)">
			<!--Moth Pupa-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2586,'Amount':50,'Enabled':true,'Type':'Purchase'}]" />
		</If>
		<If condition="not IsOnSubMap(155)">
			<LisbethTravel Area="East Shroud" XYZ="-571.7729, 8.910708, 73.77551" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="Condition.IsTimeBetween(20,21)">
			<Log Message="Waiting for window to open..." />
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Moth Pupa" condition="not HasAtLeast(7694,&Dark_Ambusher;) and HasAtLeast(2586,1) and (Condition.IsTimeBetween(21,24) or Condition.IsTimeBetween(0,3))" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" CordialType="Auto" Sit="True" SitRate="1" Mooch="2" Patience="Patience2" MinimumGPPatience="650">
				<PatientTugs>
					<PatienceTug moochLevel="0" TugType="Light" />
					<PatienceTug moochLevel="1" TugType="Light" />
				</PatientTugs>
			<FishSpots>
				<FishSpot XYZ="-586.5031, 7.944592, 86.51789" Heading="0.8807359" />
				<FishSpot XYZ="-597.1454, 7.944375, 107.6111" Heading="5.36274" />
				<FishSpot XYZ="-607.4549, 7.944794, 100.3281" Heading="5.385785" />
			</FishSpots>
			<Keepers>
				<Keeper name="Dark Ambusher" />
			</Keepers>
			<SurfaceSlaps />
		</ExFish>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>
