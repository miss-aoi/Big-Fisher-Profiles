<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Navigators_Brand "1">
]>
<Profile>
	<Name>Navigators Brand</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Navigators Brand..." />
		<If condition="not HasAtLeast(2628,3)">
			<!--Yumizuno-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2628,'Amount':3,'Enabled':true,'Type':'Goldsmith'}]" />
		</If>
		<If condition="not IsOnSubMap(220)">
			<LisbethTravel Area="Western La Noscea" XYZ="445.8791, 14.54649, 451.3748" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="IsTimeBetween(8,9)">
			<Log Message="Waiting for window to open..." />
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Yumizuno" condition="not HasAtLeast(7693,&Navigators_Brand;) and HasAtLeast(2628,1) and IsTimeBetween(9,14) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(138, 'Clear Skies', 'Fair Skies')" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" CordialType="Auto" Chum="True">
			<FishSpots>
				<FishSpot XYZ="436.388, 14.65436, 443.2291" Heading="4.019693" Sit="True" />
				<FishSpot XYZ="445.2063, 14.60643, 464.616" Heading="1.024714" Sit="True" />
				<FishSpot XYZ="451.4709, 11.39729, 456.0599" Heading="1.01638" Sit="True" />
			</FishSpots>
			<Keepers>
				<Keeper name="Navigator's Brand" />
			</Keepers>
			<SurfaceSlaps />
		</ExFish>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>