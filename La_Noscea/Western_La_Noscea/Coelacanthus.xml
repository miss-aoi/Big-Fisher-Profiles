<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Coelacanthus "1">
]>
<Profile>
	<Name>Coelacanthus</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Coelacanthus..." />
		<If condition="not HasAtLeast(2596,50)">
			<!--Spoon Worm-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2596,'Amount':50,'Enabled':true,'Type':'Purchase'}]" />
		</If>
		<If condition="not IsOnSubMap(228)">
			<LisbethTravel Area="Western La Noscea" XYZ="-464.1351, -38.0391, 737.9131" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="IsTimeBetween(21,22)">
			<Log Message="Waiting for window to open..." />
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Spoon Worm" condition="not HasAtLeast(8755,&Coelacanthus;) and HasAtLeast(2596,1) and (IsTimeBetween(22,24) or IsTimeBetween(0,3)) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(138, 'Clouds', 'Fog', 'Wind')" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" CordialType="Auto" Sit="True" SitRate="1" Mooch="2" Patience="Patience2" MinimumGPPatience="650">
			<PatientTugs>
				<PatienceTug moochLevel="0" TugType="Light" />
				<PatienceTug moochLevel="1" TugType="Medium" />
			</PatientTugs>
			<FishSpots>
				<FishSpot XYZ="-453.0573, -37.94336, 745.8951" Heading="0.1122043" />
				<FishSpot XYZ="-475.5448, -38.35024, 768.3134" Heading="6.152446" />
				<FishSpot XYZ="-490.0315, -37.92073, 761.21" Heading="5.404744" />
			</FishSpots>
			<Keepers>
				<Keeper name="Coelacanthus" />
			</Keepers>
			<SurfaceSlaps />
		</ExFish>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>