<?xml version='1.0' encoding='utf8'?>
<!--
	Profile: The Salter (7925)
	Authors: miss-aoi
-->
<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY The_Salter "1">
]>
<Profile>
	<Name>The Salter</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: The Salter..." />
		<If condition="not HasAtLeast(2599,50)">
			<!--Honey Worm-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2599,'Amount':50,'Enabled':true,'Type':'Purchase'}]" />
		</If>
		<If condition="not IsOnSubMap(216)">
			<LisbethTravel Area="Eastern La Noscea" XYZ="466.6412, 8.396084, 76.86406" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="Condition.IsTimeBetween(16,17)">
			<Log Message="Waiting for window to open..." />
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Honey Worm" condition="not HasAtLeast(7925,&The_Salter;) and HasAtLeast(2599,1) and Condition.IsTimeBetween(17,20)"
				MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" CordialType="Auto" Sit="True" SitRate="1" Chum="True">
			<FishSpots>
				<FishSpot XYZ="468.0401, 8.434704, 76.06626" Heading="2.070008" />
				<FishSpot XYZ="469.8491, 8.482638, 88.38069" Heading="3.607294" />
				<FishSpot XYZ="494.2133, 8.734549, 67.54633" Heading="0.07195473" />
			</FishSpots>
			<Keepers>
				<Keeper name="The Salter" />
			</Keepers>
			<SurfaceSlaps />
		</ExFish>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>