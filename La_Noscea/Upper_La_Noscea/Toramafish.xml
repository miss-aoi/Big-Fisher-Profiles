<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Toramafish "1">
]>
<Profile>
	<Name>Toramafish</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Toramafish..." />
		<If condition="not HasAtLeast(2626,3)">
			<!--Spinnerbait-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2626,'Amount':3,'Enabled':true,'Type':'Goldsmith'}]" />
		</If>
		<If condition="not IsOnSubMap(231)">
			<LisbethTravel Area="Upper La Noscea" XYZ="-346.4047, -1.910502, 133.8583" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="Condition.IsTimeBetween(16,17)">
			<Log Message="Waiting for window to open..." />
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Spinnerbait" condition="not HasAtLeast(7905,&Toramafish;) and HasAtLeast(2626,1) and Condition.IsTimeBetween(17,19.5) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(139, 'Clouds', 'Fog')"
				MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" CordialType="Auto" Sit="True" SitRate="1"
				Mooch="2" Patience="Patience2" MinimumGPPatience="650">
			<PatientTugs>
				<PatienceTug moochLevel="0" TugType="Light" />
				<PatienceTug moochLevel="1" TugType="Medium" />
			</PatientTugs>
			<FishSpots>
				<FishSpot XYZ="-323.3856, -2.390284, 117.6317" Heading="2.751819" />
				<FishSpot XYZ="-352.9603, -3.009619, 116.2184" Heading="3.380132" />
				<FishSpot XYZ="-365.8256, -1.967316, 130.2256" Heading="4.785471" />
			</FishSpots>
			<Keepers>
				<Keeper name="Toramafish" />
			</Keepers>
			<SurfaceSlaps />
		</ExFish>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>