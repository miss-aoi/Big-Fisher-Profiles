<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Octomammoth "1">
]>
<Profile>
	<Name>Octomammoth</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Octomammoth..." />
		<If condition="not HasAtLeast(2587,50)">
			<!--Pill Bug-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2587,'Amount':50,'Enabled':true,'Type':'Purchase'}]" />
		</If>
		<If condition="not IsOnMap(129) or not IsOnSubMap(739)">
			<LisbethTravel Area="Limsa Lominsa (Lower Decks)" XYZ="-183.9088, 2, 194.1954" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="IsTimeBetween(8,9)">
			<Log Message="Waiting for window to open..." />
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Pill Bug" condition="not HasAtLeast(7707,&Octomammoth;) and HasAtLeast(2587,1) and IsTimeBetween(9,17)" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" CordialType="Auto" Sit="True" Mooch="2" Patience="Patience2" MinimumGPPatience="650">
			<PatientTugs>
				<PatienceTug moochLevel="0" TugType="Medium" />
				<PatienceTug moochLevel="1" TugType="Medium" />
			</PatientTugs>
			<FishSpots>
				<FishSpot XYZ="-170.0204, 1.999995, 221.3403" Heading="2.986306" />
				<FishSpot XYZ="-182.248, 1.999995, 200.2238" Heading="1.683593" />
				<FishSpot XYZ="-192.3636, 0.9999907, 219.1647" Heading="5.947781" />
			</FishSpots>
			<Keepers>
				<Keeper name="Octomammoth" />
			</Keepers>
			<SurfaceSlaps />
		</ExFish>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>