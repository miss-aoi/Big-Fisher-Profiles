<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Zalera "1">
]>
<Profile>
	<Name>Zalera</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Zalera..." />
		<If condition="not HasAtLeast(2591,50)">
			<!--Rat Tail-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2591,'Amount':50,'Enabled':true,'Type':'Purchase'}]" />
		</If>
		<If condition="not IsOnSubMap(162)">
			<LisbethTravel Area="Middle La Noscea" XYZ="-102.2953, 41.2394, 9.037899" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="IsTimeBetween(8,9)">
			<Log Message="Waiting for window to open..." />
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Rat Tail" condition="not HasAtLeast(7678,&Zalera;) and HasAtLeast(2591,1) and IsTimeBetween(9,14)" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" CordialType="Auto" Sit="True" Chum="True">
			<FishSpots>
				<FishSpot XYZ="-117.8397, 38.76151, 31.36633" Heading="0.02024531" />
				<FishSpot XYZ="-141.294, 40.81226, 9.837191" Heading="5.185024" />
				<FishSpot XYZ="-158.3341, 37.78846, -16.56896" Heading="5.172457" />
			</FishSpots>
			<Keepers>
				<Keeper name="Zalera" />
			</Keepers>
			<SurfaceSlaps />
		</ExFish>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>