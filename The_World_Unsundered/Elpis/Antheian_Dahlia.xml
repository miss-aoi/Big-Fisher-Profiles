<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Antheian_Dahlia "1">
]>
<Profile>
	<Name>Antheian Dahlia</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Antheian Dahlia..." />
		<If condition="not HasAtLeast(36591,50)">
			<!--Mayfly-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':36591,'Amount':50,'Enabled':true,'Type':'Exchange'}]" />
		</If>
		<If condition="not IsOnSubMap(3947)">
			<LisbethTravel Area="Elpis (Philomythes Notos)" XYZ="495.3606, 138.5854, -212.613" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="IsTimeBetween(8,9)">
			<Log Message="Waiting for window to open..." />
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Mayfly" condition="not HasAtLeast(38836,&Antheian_Dahlia;) and HasAtLeast(36591,1) and IsTimeBetween(9,14) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(961, 'Clear Skies') and ExBuddy.Helpers.SkywatcherPlugin.PredictWeatherInZone(961, TimeSpan.FromHours(-8), 'Clouds')" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" CordialType="Auto" Sit="True" Chum="True">
			<FishSpots>
				<FishSpot XYZ="495.3606, 138.5854, -212.613" Heading="0.708328" />
				<FishSpot XYZ="463.4649, 138.5854, -187.2824" Heading="5.602932" />
				<FishSpot XYZ="508.3931, 138.5854, -183.9252" Heading="1.910515" />
			</FishSpots>
			<Keepers>
				<Keeper name="Antheian Dahlia" />
			</Keepers>
			<SurfaceSlaps />
		</ExFish>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>