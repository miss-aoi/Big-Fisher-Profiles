<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Hermits_End "1">
]>
<Profile>
	<Name>Hermit's End</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Hermit's End..." />
		<If condition="not HasAtLeast(20675,50)">
			<!--Stonefly Larva-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':20675,'Amount':50,'Enabled':true,'Type':'Exchange'}]" />
		</If>
		<If condition="not IsOnSubMap(2752)">
			<LisbethTravel Area="Yanxia" XYZ="415.9842, 0.7849607, -271.2573" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="IsTimeBetween(19,20)">
			<WaitTimer WaitTime="30" />
		</While>
		<While condition="not HasAtLeast(24887,&Hermits_End;) and HasAtLeast(20675,1) and IsTimeBetween(20,24) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(614, 'Clear Skies')">
			<ExFish Bait="Stonefly Larva" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" Cordial="Auto" Sit="True" Chum="True">
				<FishSpots>
					<FishSpot XYZ="411.9119, -0.3000069, -275.5999" Heading="3.849829" />
					<FishSpot XYZ="393.0524, -0.3000069, -275.2616" Heading="3.026734" />
					<FishSpot XYZ="419.3833, -0.3000069, -301.45" Heading="5.332651" />
				</FishSpots>
				<Keepers>
					<Keeper name="Hermit's End" />
				</Keepers>
				<SurfaceSlaps />
			</ExFish>
		</While>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>