<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Garden_Skipper "1">
]>
<Profile>
	<Name>Garden Skipper</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Garden Skipper..." />
		<If condition="not HasAtLeast(20615,50)">
			<!--Midge Larva-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':20615,'Amount':50,'Enabled':true,'Type':'Purchase'}]" />
		</If>
		<If condition="not IsOnSubMap(2756)">
			<LisbethTravel Area="The Azim Steppe" XYZ="494.108, -23.48081, 306.5688" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="IsTimeBetween(7,8)">
			<WaitTimer WaitTime="30" />
		</While>
		<While condition="not HasAtLeast(24892,&Garden_Skipper;) and HasAtLeast(20615,1) and IsTimeBetween(8,12) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(622, 'Clear Skies') and ExBuddy.Helpers.SkywatcherPlugin.PredictWeatherInZone(622, TimeSpan.FromHours(-8), 'Fair Skies')">
			<ExFish Bait="Midge Larva" condition="not HasAtLeast(24892,&Garden_Skipper;) and HasAtLeast(20615,1) and IsTimeBetween(8,12) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(622, 'Clear Skies') and ExBuddy.Helpers.SkywatcherPlugin.PredictWeatherInZone(622, TimeSpan.FromHours(-8), 'Fair Skies')" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" Cordial="Auto" Sit="True" Chum="True">
				<FishSpots>
					<FishSpot XYZ="488.0088, -25.04177, 299.4184" Heading="4.25456" />
					<FishSpot XYZ="480.9908, -26.60763, 320.3468" Heading="6.181407" />
					<FishSpot XYZ="478.5235, -28.58864, 355.2843" Heading="0.02180147" />
				</FishSpots>
				<Keepers>
					<Keeper name="Garden Skipper" />
				</Keepers>
				<SurfaceSlaps />
			</ExFish>
		</While>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>