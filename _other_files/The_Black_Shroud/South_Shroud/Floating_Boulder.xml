<?xml version='1.0' encoding='utf8'?>
<!--
	Profile: Floating Boulder (7913)
	Authors: miss-aoi
-->
<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Floating_Boulder "1">
]>
<Profile>
	<Name>Floating Boulder</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Floating Boulder..." />
		<If condition="not HasAtLeast(2595,50)">
			<!--Bass Ball-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':TEMP,'Amount':50,'Enabled':true,'Type':'Purchase'}]" />
		</If>
		<If condition="not IsOnSubMap(79)">
			<LisbethTravel Area="South Shroud" XYZ="-164.9751, 2.177883, 171.357" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="IsTimeBetween(-1,0)">
			<WaitTimer WaitTime="30" />
		</While>
		<While condition="not HasAtLeast(7913,&Floating_Boulder;) and HasAtLeast(2595,1) and IsTimeBetween(0,8) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(153, 'Clouds', 'Fog')">
			<ExFish Bait="Bass Ball" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" Cordial="Auto" Chum="True">
				<FishSpots>
					<FishSpot XYZ="-141.2495, -0.2347637, 164.7361" Heading="3.100041" />
					<FishSpot XYZ="-165.4892, -0.2811633, 158.3398" Heading="3.567091" />
					<FishSpot XYZ="-173.4589, -0.2794424, 147.2547" Heading="0.2600002" />
				</FishSpots>
				<Keepers>
					<Keeper name="Floating Boulder" />
				</Keepers>
				<SurfaceSlaps />
			</ExFish>
		</While>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>