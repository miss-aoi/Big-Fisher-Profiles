<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Imperial_Goldfish "1">
]>
<Profile>
	<Name>Imperial Goldfish</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Imperial Goldfish..." />
		<If condition="not HasAtLeast(2599,50)">
