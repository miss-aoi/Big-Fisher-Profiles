		<If condition="not IsOnSubMap(168)">
			<LisbethTravel Area="Eastern La Noscea" XYZ="-253.4813, 35.16173, 289.4978" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="not HasAtLeast(8752,&Imperial_Goldfish;) and HasAtLeast(2599,1)">
			<ExFish Bait="Honey Worm" Condition="not Core.Player.HasAura(568)" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" Cordial="Auto" Mooch="3" Patience="Patience2" MinimumGPPatience="650">
