<?xml version='1.0' encoding='utf8'?>
<!--
	Profile: Sabertooth Cod (7691)
	Authors: miss-aoi
-->
<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Sabertooth_Cod "1">
]>
<Profile>
	<Name>Sabertooth Cod</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Sabertooth Cod..." />
		<If condition="not HasAtLeast(2589,50)">
			<!--Goby Ball-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':TEMP,'Amount':50,'Enabled':true,'Type':'Purchase'}]" />
		</If>
		<If condition="not IsOnSubMap(339)">
			<LisbethTravel Area="Lower La Noscea" XYZ="-97.83857, 4.310742, 853.5196" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="IsTimeBetween(15,16)">
			<WaitTimer WaitTime="30" />
		</While>
		<While condition="not HasAtLeast(7691,&Sabertooth_Cod;) and HasAtLeast(2589,1) and IsTimeBetween(16,22)">
			<ExFish Bait="Goby Ball" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" Cordial="Auto" Chum="True">
				<FishSpots>
					<FishSpot XYZ="-132.2859, 4.310742, 860.4258" Heading="0.04101062" />
					<FishSpot XYZ="-79.52073, 5.327857, 860.4695" Heading="6.221565" />
					<FishSpot XYZ="-93.51559, 4.310742, 846.0161" Heading="3.272657" />
				</FishSpots>
				<Keepers>
					<Keeper name="Sabertooth Cod" />
				</Keepers>
				<SurfaceSlaps />
			</ExFish>
		</While>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>