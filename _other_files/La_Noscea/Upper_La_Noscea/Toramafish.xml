<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Toramafish "1">
]>
<Profile>
	<Name>Toramafish</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Toramafish..." />
		<If condition="not HasAtLeast(2626,50)">
			<!--Spinnerbait-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':TEMP,'Amount':50,'Enabled':true,'Type':'Purchase'}]" />
		</If>
		<If condition="not IsOnSubMap(231)">
			<LisbethTravel Area="Upper La Noscea" XYZ="-346.4047, -1.910502, 133.8583" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="IsTimeBetween(16,17)">
			<WaitTimer WaitTime="30" />
		</While>
		<While condition="not HasAtLeast(7905,&Toramafish;) and HasAtLeast(2626,1) and IsTimeBetween(17,20) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(139, 'Clouds', 'Fog')">
			<ExFish Bait="Spinnerbait" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" Cordial="Auto" Mooch="2" Patience="Patience2" MinimumGPPatience="650">
				<PatientTugs>
					<PatienceTug moochLevel="0" TugType="Light" />
					<PatienceTug moochLevel="1" TugType="Heavy" />
				</PatientTugs>
				<FishSpots>
					<FishSpot XYZ="-323.3856, -2.390284, 117.6317" Heading="2.751819" />
					<FishSpot XYZ="-352.9603, -3.009619, 116.2184" Heading="3.380132" />
					<FishSpot XYZ="-365.8256, -1.967316, 130.2256" Heading="4.785471" />
				</FishSpots>
				<Keepers>
					<Keeper name="Toramafish" />
				</Keepers>
				<SurfaceSlaps />
			</ExFish>
		</While>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>