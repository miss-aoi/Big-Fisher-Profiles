<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Winged_Dame "1">
]>
<Profile>
	<Name>Winged Dame</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Winged Dame..." />
		<If condition="not HasAtLeast(27590,50)">
			<!--Squid Strip-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':27590,'Amount':50,'Enabled':true,'Type':'Exchange'}]" />
		</If>
		<If condition="not IsOnSubMap(3258)">
			<LisbethTravel Area="Eulmore" XYZ="44.8386, -23.72356, 106.9456" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="IsTimeBetween(11,12)">
			<WaitTimer WaitTime="30" />
		</While>
		<While condition="not HasAtLeast(28066,&Winged_Dame;) and HasAtLeast(27590,1) and IsTimeBetween(12,19) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(820, 'Clear Skies')">
			<ExFish Bait="Squid Strip" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" Cordial="Auto" Sit="True" Chum="True">
				<FishSpots>
					<FishSpot XYZ="44.8386, -23.72356, 106.9456" Heading="6.260114" />
					<FishSpot XYZ="23.39771, -23.74639, 108.9064" Heading="0.8209686" />
					<FishSpot XYZ="30.68819, -23.75438, 124.3001" Heading="1.349121" />
				</FishSpots>
				<Keepers>
					<Keeper name="Winged Dame" />
				</Keepers>
				<SurfaceSlaps />
			</ExFish>
		</While>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>