<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Aquamaton "1">
]>
<Profile>
	<Name>Aquamaton</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Aquamaton..." />
		<If condition="not HasAtLeast(27588,50)">
			<!--Short Bill Minnow-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':27588,'Amount':3,'Enabled':true,'Type':'Exchange'}]" />
		</If>
		<If condition="not HasAtLeast(27590,50)">
			<!--Short Bill Minnow-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':27590,'Amount':50,'Enabled':true,'Type':'Purchase'}]" />
		</If>
		<If condition="not IsOnSubMap(3090)">
			<LisbethTravel Area="Kholusia" XYZ="-218.3601, 8.98271, 765.7238" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="IsTimeBetween(9,10)">
			<WaitTimer WaitTime="30" />
		</While>
		<While condition="not HasAtLeast(33240,&Aquamaton;) and HasAtLeast(27588,1) and IsTimeBetween(10,16) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(814, 'Gales') and ExBuddy.Helpers.SkywatcherPlugin.PredictWeatherInZone(814, TimeSpan.FromHours(-8), 'Gales')">
			<ExFish Bait="Squid Strip" Condition="not Core.Player.HasAura(568)" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" Cordial="Auto" Sit="True" Chum="True">
				<FishSpots>
					<FishSpot XYZ="-218.0921, 8.740814, 760.5208" Heading="3.420164" />
					<FishSpot XYZ="-197.7823, 11.86121, 750.546" Heading="4.203466" />
					<FishSpot XYZ="-248.2603, 11.9524, 764.6677" Heading="3.401308" />
				</FishSpots>
				<Keepers>
					<Keeper name="Shadeshifter" />
				</Keepers>
			</ExFish>
			<ExFish Bait="Short Bill Minnow" Condition="Core.Player.HasAura(568)" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" Cordial="Auto" Sit="True" Chum="True">
				<FishSpots>
					<FishSpot XYZ="-218.0921, 8.740814, 760.5208" Heading="3.420164" />
					<FishSpot XYZ="-197.7823, 11.86121, 750.546" Heading="4.203466" />
					<FishSpot XYZ="-248.2603, 11.9524, 764.6677" Heading="3.401308" />
				</FishSpots>
				<Keepers>
					<Keeper name="Aquamaton" />
				</Keepers>
				<SurfaceSlaps />
			</ExFish>
		</While>
		<If condition="HasAtLeast(33319,1)">
			<Desynth ItemIds="33319" DesynthDelay="6000" DesynthTimeout="10" />
		</If>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>