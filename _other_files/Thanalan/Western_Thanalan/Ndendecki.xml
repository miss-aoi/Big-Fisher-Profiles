<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Ndendecki "1">
]>
<Profile>
	<Name>Ndendecki</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Ndendecki..." />
		<If condition="not HasAtLeast(2599,50)">
			<!--Honey Worm-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2599,'Amount':50,'Enabled':true,'Type':'Purchase'}]" />
		</If>
		<If condition="not IsOnSubMap(275)">
			<LisbethTravel Area="Western Thanalan" XYZ="-220.5043, 15.77091, -736.3502" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="IsTimeBetween(17,18)">
			<WaitTimer WaitTime="30" />
		</While>
		<While condition="not HasAtLeast(8765,&Ndendecki;) and HasAtLeast(2599,1) and (IsTimeBetween(18,24) or IsTimeBetween(0,5)) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(140, 'Fog')">
			<ExFish Bait="Honey Worm" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" Cordial="Auto" Sit="True" Mooch="3" Patience="Patience2" MinimumGPPatience="650">
				<PatientTugs>
					<PatienceTug moochLevel="0" TugType="Light" />
					<PatienceTug moochLevel="1" TugType="Medium" />
					<PatienceTug moochLevel="2" TugType="Heavy" />
				</PatientTugs>
				<FishSpots>
					<FishSpot XYZ="-176.5261, 14.63993, -720.3877" Heading="3.323525" />
					<FishSpot XYZ="-180.7458, 14.63993, -742.4382" Heading="0.525413" />
					<FishSpot XYZ="-212.2421, 14.67033, -736.3536" Heading="1.601916" />
				</FishSpots>
				<Keepers>
					<Keeper name="Ndendecki" />
				</Keepers>
				<SurfaceSlaps />
			</ExFish>
		</While>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>