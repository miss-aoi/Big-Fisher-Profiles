<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Dark_Knight "1">
]>
<Profile>
	<Name>Dark Knight</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Dark Knight..." />
		<If condition="not HasAtLeast(2614,50)">
			<!--Crow Fly-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':TEMP,'Amount':50,'Enabled':true,'Type':'Purchase'}]" />
		</If>
		<If condition="not IsOnSubMap(249)">
			<LisbethTravel Area="Central Thanalan" XYZ="263.259, -19.71835, -135.9717" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="not HasAtLeast(7689,&Dark_Knight;) and HasAtLeast(2614,1) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(141, 'Clouds', 'Dust Storms', 'Fog')">
			<ExFish Bait="Crow Fly" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" Cordial="Auto" Chum="True">
				<FishSpots>
					<FishSpot XYZ="270.3185, -19.71835, -141.46" Heading="3.679023" />
					<FishSpot XYZ="274.0152, -19.71835, -161.9428" Heading="0.4138639" />
					<FishSpot XYZ="293.4185, -19.05349, -153.5947" Heading="3.630853" />
				</FishSpots>
				<Keepers>
					<Keeper name="Dark Knight" />
				</Keepers>
				<SurfaceSlaps />
			</ExFish>
		</While>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>