					<FishSpot XYZ="118.6457, 12.16843, 221.7863" Heading="1.835743" />
					<FishSpot XYZ="132.7265, 19.5851, 207.0147" Heading="6.229785" />
				</FishSpots>
				<Keepers>
					<Keeper name="Hannibal" />
				</Keepers>
