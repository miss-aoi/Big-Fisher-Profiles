<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Basking_Shark "1">
]>
<Profile>
	<Name>Basking Shark</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Basking Shark..." />
		<If condition="not HasAtLeast(28634,50)">
			<!--Metal Spinner-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':28634,'Amount':3,'Enabled':true,'Type':'Purchase'}]" />
		</If>
		<If condition="not IsOnSubMap(2129)">
			<LisbethTravel Area="The Sea of Clouds" XYZ="-342.964, 20.93506, -820.5392" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="not HasAtLeast(16743,&Basking_Shark;) and HasAtLeast(28634,1) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(401, 'Clear Skies') and ExBuddy.Helpers.SkywatcherPlugin.PredictWeatherInZone(401, TimeSpan.FromHours(-8), 'Clear Skies')">
			<ExFish Bait="Metal Spinner" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" Cordial="Auto" Mooch="2" Patience="Patience2" MinimumGPPatience="650">
				<PatientTugs>
					<PatienceTug moochLevel="0" TugType="Light" />
					<PatienceTug moochLevel="1" TugType="Heavy" />
				</PatientTugs>
				<FishSpots>
					<FishSpot XYZ="-353.6552, 19.58649, -832.2133" Heading="3.856718" />
					<FishSpot XYZ="-339.0728, 19.71094, -849.5245" Heading="4.007522" />
					<FishSpot XYZ="-322.0511, 20.56616, -850.5399" Heading="1.887997" />
				</FishSpots>
				<Keepers>
					<Keeper name="Basking Shark" />
				</Keepers>
				<SurfaceSlaps />
			</ExFish>
		</While>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>