					<FishSpot XYZ="-2.573665, -1.96711, 109.1576" Heading="0.574614" />
					<FishSpot XYZ="5.053344, -1.601119, 141.7926" Heading="3.219832" />
				</FishSpots>
				<Keepers>
					<Keeper name="Hookstealer" />
				</Keepers>
				<SurfaceSlaps />
