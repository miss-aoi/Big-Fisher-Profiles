<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Fingers "1">
]>
<Profile>
	<Name>Fingers</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Fingers..." />
		<If condition="not HasAtLeast(2606,50)">
			<!--Northern Krill-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2606,'Amount':50,'Enabled':true,'Type':'Purchase'}]" />
		</If>
		<If condition="not IsOnSubMap(370)">
			<LisbethTravel Area="Western Thanalan" XYZ="-328.2706, 13.9835, -100.0699" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="IsTimeBetween(16,17)">
			<Log Message="Waiting for window to open..." />
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Northern Krill" condition="not HasAtLeast(7906,&Fingers;) and HasAtLeast(2606,1) and IsTimeBetween(17,18)" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" CordialType="Auto" Chum="True">
			<FishSpots>
				<FishSpot XYZ="-324.3295, 13.9835, -99.37143" Heading="1.510842" />
				<FishSpot XYZ="-340.3997, 13.90376, -99.89254" Heading="0.01335144" />
				<FishSpot XYZ="-356.0331, 13.90376, -100.7579" Heading="4.72364" />
			</FishSpots>
			<Keepers>
				<Keeper name="Fingers" />
			</Keepers>
			<SurfaceSlaps />
		</ExFish>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>