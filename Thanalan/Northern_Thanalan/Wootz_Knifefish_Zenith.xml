				<FishSpot XYZ="-69.34203, 45.6293, 41.65791" Heading="5.809361" Sit="True" />
				<FishSpot XYZ="-61.20588, 45.9164, 43.82962" Heading="5.972723" Sit="True" />
			</FishSpots>
			<Keepers>
				<Keeper name="Wootz Knifefish Zenith" />
			</Keepers>
