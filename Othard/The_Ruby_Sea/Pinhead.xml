<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Pinhead "1">
]>
<Profile>
	<Name>Pinhead</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Pinhead..." />
		<If condition="not HasAtLeast(20617,50)">
			<!--Live Shrimp-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':20617,'Amount':50,'Enabled':true,'Type':'Purchase'}]" />
		</If>
		<If condition="not IsOnSubMap(2751)">
			<LisbethTravel Area="The Ruby Sea" XYZ="-735.6036, 1.035747, -480.6779" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="Condition.IsTimeBetween(15,16)">
			<Log Message="Waiting for window to open..." />
			<WaitTimer WaitTime="30" />
		</While>
			<ExFish Bait="Live Shrimp" condition="not HasAtLeast(24884,&Pinhead;) and HasAtLeast(20617,1) and Condition.IsTimeBetween(16,24) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(613, 'Thunder') and ExBuddy.Helpers.SkywatcherPlugin.PredictWeatherInZone(613, TimeSpan.FromHours(-8), 'Clear Skies', 'Fair Skies')" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" CordialType="Auto" Sit="True" SitRate="1" Chum="True">
				<FishSpots>
					<FishSpot XYZ="-715.4233, -0.1, -459.959" Heading="1.411656" />
					<FishSpot XYZ="-713.6649, -0.1, -494.2176" Heading="1.903837" />
					<FishSpot XYZ="-659.5365, 1.151, -481.1787" Heading="1.587571" />
				</FishSpots>
				<Keepers>
					<Keeper name="Pinhead" />
				</Keepers>
				<SurfaceSlaps />
			</ExFish>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>