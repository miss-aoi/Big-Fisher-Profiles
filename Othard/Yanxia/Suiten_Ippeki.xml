]>
<Profile>
	<Name>Suiten Ippeki</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Suiten Ippeki..." />
		<If condition="not HasAtLeast(20675,50)">
			<!--Stonefly Larva-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':20675,'Amount':50,'Enabled':true,'Type':'Exchange'}]" />
		</If>
		<If condition="not IsOnSubMap(2809)">
			<LisbethTravel Area="Yanxia (Fierce)" XYZ="-369.1738, 1.230016, -377.8241" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="IsTimeBetween(15,16)">
			<WaitTimer WaitTime="30" />
		</While>
		<While condition="not HasAtLeast(24888,&Suiten_Ippeki;) and HasAtLeast(20675,1) and IsTimeBetween(16,24) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(614, 'Showers') and ExBuddy.Helpers.SkywatcherPlugin.PredictWeatherInZone(614, TimeSpan.FromHours(-8), 'Fair Skies')">
			<ExFish Bait="Stonefly Larva" condition="not HasAtLeast(24888,&Suiten_Ippeki;) and HasAtLeast(20675,1) and IsTimeBetween(16,24) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(614, 'Showers') and ExBuddy.Helpers.SkywatcherPlugin.PredictWeatherInZone(614, TimeSpan.FromHours(-8), 'Fair Skies')" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" Cordial="Auto" Sit="True" Chum="True">
				<FishSpots>
					<FishSpot XYZ="-372.2944, -0.2054873, -374.6753" Heading="5.421563" />
					<FishSpot XYZ="-363.5536, 1.230066, -357.805" Heading="6.093866" />
					<FishSpot XYZ="-349.8077, -0.4114811, -350.6641" Heading="4.703186" />
				</FishSpots>
				<Keepers>
					<Keeper name="Suiten Ippeki" />
				</Keepers>
				<SurfaceSlaps />
			</ExFish>
		</While>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>