<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Duskfish "1">
]>
<Profile>
	<Name>Duskfish</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Duskfish..." />
		<If condition="not HasAtLeast(20614,50)">
			<!--Nightcrawler-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':20614,'Amount':50,'Enabled':true,'Type':'Purchase'}]" />
		</If>
		<If condition="not IsOnSubMap(2822)">
			<LisbethTravel Area="The Azim Steppe" XYZ="97.4893, 116.043, -41.54379" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="IsTimeBetween(7,8)">
			<Log Message="Waiting for window to open..." />
			<WaitTimer WaitTime="30" />
		</While>
			<ExFish Bait="Nightcrawler" condition="not HasAtLeast(24217,&Duskfish;) and HasAtLeast(20614,1) and IsTimeBetween(8,16) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(622, 'Clouds') and ExBuddy.Helpers.SkywatcherPlugin.PredictWeatherInZone(622, TimeSpan.FromHours(-8), 'Rain', 'Gales')" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" CordialType="HiCordial" Sit="True" Mooch="2" Patience="Patience2" MinimumGPPatience="650">
				<PatientTugs>
					<PatienceTug moochLevel="0" TugType="Light" />
					<PatienceTug moochLevel="1" TugType="Medium" />
				</PatientTugs>
				<FishSpots>
					<FishSpot XYZ="96.2347, 116.043, -49.97553" Heading="3.73935" />
					<FishSpot XYZ="104.942, 116.043, -48.41993" Heading="2.447108" />
					<FishSpot XYZ="101.5779, 116.043, -35.35352" Heading="0.8574641" />
				</FishSpots>
				<Keepers>
					<Keeper name="Duskfish" />
				</Keepers>
				<SurfaceSlaps />
			</ExFish>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>