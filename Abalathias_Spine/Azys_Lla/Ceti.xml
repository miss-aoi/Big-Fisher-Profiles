<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Ceti "1">
]>
<Profile>
	<Name>Ceti</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Ceti..." />
		<If condition="not HasAtLeast(12710,50)">
			<!--Fiend Worm-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':12710,'Amount':50,'Enabled':true,'Type':'Exchange'}]" />
		</If>
		<If condition="not IsOnSubMap(2177)">
			<LisbethTravel Area="Azys Lla" XYZ="-754.7177, 26.048, -292.8931" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="Condition.IsTimeBetween(17,18)">
			<Log Message="Waiting for window to open..." />
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Fiend Worm" condition="not HasAtLeast(17580,&Ceti;) and HasAtLeast(12710,1) and Condition.IsTimeBetween(18,22) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(402, 'Thunder') and ExBuddy.Helpers.SkywatcherPlugin.PredictWeatherInZone(402, TimeSpan.FromHours(-8), 'Clouds')"
				MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" CordialType="Auto" Sit="True" SitRate="1" Chum="True">
			<FishSpots>
				<FishSpot XYZ="-761.8911, 25.44802, -291.2364" Heading="4.815927" />
				<FishSpot XYZ="-763.5567, 26.9576, -303.4038" Heading="5.268315" />
				<FishSpot XYZ="-762.8005, 24.58127, -282.8617" Heading="4.108016" />
			</FishSpots>
			<Keepers>
				<Keeper name="Ceti" />
			</Keepers>
			<SurfaceSlaps />
		</ExFish>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>