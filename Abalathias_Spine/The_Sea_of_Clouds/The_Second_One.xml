			<ChangeClass Job="Fisher" />
		</If>
		<While condition="not HasAtLeast(15636,&The_Second_One;) and HasAtLeast(12712,1) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(401, 'Wind')">
