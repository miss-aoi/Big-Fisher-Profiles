					<FishSpot XYZ="-742.3828, -99.95825, -636.5696" Heading="0.141259" />
					<FishSpot XYZ="-759.2081, -99.72325, -640.7861" Heading="5.967869" />
				</FishSpots>
				<Keepers>
					<Keeper name="The Second One" />
				</Keepers>
