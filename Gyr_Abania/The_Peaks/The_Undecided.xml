				<FishSpot XYZ="109.8604, 114.3518, -565.3806" Heading="2.454573" />
				<FishSpot XYZ="150.3323, 114.3518, -577.166" Heading="4.136375" />
				<FishSpot XYZ="128.5622, 114.3333, -618.4976" Heading="5.072576" />
			</FishSpots>
			<Keepers>
				<Keeper name="The Undecided" />
			</Keepers>
