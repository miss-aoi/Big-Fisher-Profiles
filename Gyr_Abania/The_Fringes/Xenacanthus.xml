<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Xenacanthus "1">
]>
<Profile>
	<Name>Xenacanthus</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Xenacanthus..." />
		<If condition="not HasAtLeast(20675,50)">
			<!--Stonefly Larva-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':20675,'Amount':50,'Enabled':true,'Type':'Exchange'}]" />
		</If>
		<If condition="not IsOnSubMap(2600)">
			<LisbethTravel Area="The Fringes" XYZ="-499.0072, 82.00081, -262.1857" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="IsTimeBetween(15,16)">
			<Log Message="Waiting for window to open..." />
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Stonefly Larva" condition="not HasAtLeast(24990,&Xenacanthus;) and HasAtLeast(20675,1) and IsTimeBetween(16,20)" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" CordialType="Auto" Mooch="1" Patience="Patience2" MinimumGPPatience="650">
			<PatientTugs>
				<PatienceTug moochLevel="0" TugType="Heavy" />
			</PatientTugs>
			<FishSpots>
				<FishSpot XYZ="-500.2141, 80.50189, -246.2518" Heading="0.8887141" />
				<FishSpot XYZ="-490.678, 81.47359, -265.1501" Heading="1.012286" />
				<FishSpot XYZ="-487.4555, 82.28168, -237.0961" Heading="3.938151" />
			</FishSpots>
			<Keepers>
				<Keeper name="Xenacanthus" />
			</Keepers>
			<SurfaceSlaps />
		</ExFish>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>