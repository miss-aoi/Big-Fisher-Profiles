<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY The_Archbishop "1">
]>
<Profile>
	<Name>The Archbishop</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: The Archbishop..." />
		<If condition="not HasAtLeast(20619,3)">
			<!--Suspending Minnow-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':20619,'Amount':3,'Enabled':true,'Type':'Exchange'}]" />
		</If>
		<If condition="not IsOnSubMap(2624)">
			<LisbethTravel Area="The Fringes" XYZ="-383.9916, 38.67, 497.0901" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="Condition.IsTimeBetween(11,12)">
			<Log Message="Waiting for window to open..." />
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Suspending Minnow" condition="not HasAtLeast(23059,&The_Archbishop;) and HasAtLeast(20619,1) and Condition.IsTimeBetween(12,16)" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" CordialType="Auto" Sit="True" SitRate="1" Chum="True">
			<FishSpots>
				<FishSpot XYZ="-383.9916, 38.67, 497.0901" Heading="1.470778" />
				<FishSpot XYZ="-353.8622, 38.67, 488.2345" Heading="4.25423" />
				<FishSpot XYZ="-365.606, 38.67, 473.2102" Heading="0.4759319" />
			</FishSpots>
			<Keepers>
				<Keeper name="The Archbishop" />
			</Keepers>
			<SurfaceSlaps />
		</ExFish>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>