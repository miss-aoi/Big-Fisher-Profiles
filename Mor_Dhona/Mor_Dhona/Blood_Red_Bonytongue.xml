<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Blood_Red_Bonytongue "1">
]>
<Profile>
	<Name>Blood Red Bonytongue</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Blood Red Bonytongue..." />
		<If condition="not HasAtLeast(2599,50)">
			<!--Honey Worm-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2599,'Amount':50,'Enabled':true,'Type':'Purchase'}]" />
		</If>
		<If condition="not IsOnSubMap(410)">
			<LisbethTravel Area="Mor Dhona" XYZ="543.8335, -9.70342, -730.1382" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="Condition.IsTimeBetween(3.5,4.5)">
			<Log Message="Waiting for window to open..." />
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Honey Worm" condition="not HasAtLeast(8776,&Blood_Red_Bonytongue;) and HasAtLeast(2599,1) and Condition.IsTimeBetween(4.5,11.5) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(156, 'Fog') and ExBuddy.Helpers.SkywatcherPlugin.PredictWeatherInZone(156, TimeSpan.FromHours(-8), 'Clear Skies', 'Fair Skies')"
				MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" CordialType="Auto" Sit="True" SitRate="1"
				Mooch="2" Patience="Patience2" MinimumGPPatience="650">
			<PatientTugs>
				<PatienceTug moochLevel="0" TugType="Light" />
				<PatienceTug moochLevel="1" TugType="Medium" />
			</PatientTugs>
			<FishSpots>
				<FishSpot XYZ="315.7721, 22.64252, -673.6241" Heading="1.17469" />
				<FishSpot XYZ="320.1711, 22.98494, -678.5112" Heading="0.7348647" />
				<FishSpot XYZ="334.5092, 25.11681, -679.5267" Heading="1.212389" />
			</FishSpots>
			<Keepers>
				<Keeper name="Blood Red Bonytongue" />
			</Keepers>
			<SurfaceSlaps />
		</ExFish>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>