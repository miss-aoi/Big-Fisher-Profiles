<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Forbiddingway "1">
]>
<Profile>
	<Name>Forbiddingway</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Forbiddingway..." />
		<If condition="not HasAtLeast(36597,50)">
			<!--Stardust-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':36597,'Amount':50,'Enabled':true,'Type':'Exchange'}]" />
		</If>
		<If condition="not IsOnSubMap(3970)">
			<LisbethTravel Area="Ultima Thule (Ostrakon Tria)" XYZ="-656.1792, 77.23194, 552.069" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="Condition.IsTimeBetween(7,8)">
			<Log Message="Waiting for window to open..." />
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Stardust" condition="not HasAtLeast(36685,&Forbiddingway;) and HasAtLeast(36597,1) and Condition.IsTimeBetween(8,12) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(960, 'Astromagnetic Storm')" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" CordialType="Auto" Sit="True" SitRate="1" Chum="True">
			<FishSpots>
				<FishSpot XYZ="-660.4504, 76.32266, 559.2496" Heading="5.308353" />
				<FishSpot XYZ="-664.7294, 77.29636, 549.6221" Heading="4.667468" />
				<FishSpot XYZ="-651.1329, 74.37665, 576.5045" Heading="5.811008" />
			</FishSpots>
			<Keepers>
				<Keeper name="Forbiddingway" />
			</Keepers>
			<SurfaceSlaps />
		</ExFish>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>