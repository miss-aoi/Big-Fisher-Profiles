				<FishSpots>
					<FishSpot XYZ="-620.9103, 76.91122, 458.575" Heading="0.5171647" />
					<FishSpot XYZ="-621.903, 76.91122, 478.4088" Heading="1.976967" />
					<FishSpot XYZ="-597.0199, 76.91121, 462.901" Heading="1.832453" />
				</FishSpots>
				<Keepers>
					<Keeper name="Foun Ahlm" />
				</Keepers>
				<SurfaceSlaps />
			</ExFish>
		</While>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>