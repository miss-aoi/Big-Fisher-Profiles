]>
<Profile>
	<Name>Foun Ahlm</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Foun Ahlm..." />
		<If condition="not HasAtLeast(36595,50)">
			<!--Chimera Worm-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':36595,'Amount':50,'Enabled':true,'Type':'Exchange'}]" />
		</If>
		<If condition="not IsOnSubMap(3970)">
			<LisbethTravel Area="Ultima Thule" XYZ="-620.9103, 76.91122, 458.575" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="IsTimeBetween(9,10)">
			<WaitTimer WaitTime="30" />
		</While>
		<While condition="not HasAtLeast(36684,&Foun_Ahlm;) and HasAtLeast(36595,1) and IsTimeBetween(10,14) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(960, 'Umbral Wind')">
