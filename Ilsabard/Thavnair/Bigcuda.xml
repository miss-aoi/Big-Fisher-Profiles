<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Bigcuda "1">
]>
<Profile>
	<Name>Bigcuda</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Bigcuda..." />
		<If condition="not HasAtLeast(36590,3)">
			<!--Sky Spoon Lure-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':36590,'Amount':3,'Enabled':true,'Type':'Exchange'}]" />
		</If>
		<If condition="not IsOnSubMap(3892)">
			<LisbethTravel Area="Thavnair (West)" XYZ="515.6667, 4.408312, 3.336241" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="Condition.IsTimeBetween(7,8)">
			<Log Message="Waiting for window to open..." />
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Sky Spoon Lure" condition="not HasAtLeast(37848,&Bigcuda;) and HasAtLeast(36590,1) and Condition.IsTimeBetween(8,16) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(957, 'Rain', 'Showers') and ExBuddy.Helpers.SkywatcherPlugin.PredictWeatherInZone(957, TimeSpan.FromHours(-8), 'Clear Skies', 'Fair Skies')" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" CordialType="Auto" Sit="True" SitRate="1" Chum="True">
			<FishSpots>
				<FishSpot XYZ="515.6667, 4.408312, 3.336241" Heading="5.35565" />
				<FishSpot XYZ="506.4695, 4.204235, 28.28826" Heading="5.736829" />
				<FishSpot XYZ="494.5585, 4.212761, -17.29577" Heading="4.048746" />
			</FishSpots>
			<Keepers>
				<Keeper name="Bigcuda" />
			</Keepers>
			<SurfaceSlaps />
		</ExFish>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>