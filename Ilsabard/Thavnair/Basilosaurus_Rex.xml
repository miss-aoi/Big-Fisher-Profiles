<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Basilosaurus_Rex "1">

	<!-- DO NOT CHANGE -->
	<!ENTITY TIME_WINDOW  "True">
	<!ENTITY WEATHER_PAST "True">
	<!ENTITY WEATHER_NOW  "ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(957, 'Fair Skies')">
]>
<Profile>
	<Name>Bigcuda</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Basilosaurus Rex..." />
		<If Condition="not IsOnMap(957)">
			<RunCode Name="AnglersArtToGP"/>
		</If>
		<If condition="not HasAtLeast(36593,3)">
			<!--Mackerel Strip-->
			<Log Message="Farming more Mackerel Strip with Lisbeth." />
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':36593,'Amount':3,'Enabled':true,'Type':'Exchange'}]" />
			<LLoadProfile Path="../../Start.xml" />
		</If>
		<If condition="not IsOnSubMap(3892)">
			<LisbethTravel Area="Thavnair (West)" XYZ="515.6667, 4.408312, 3.336241" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<ExFish Bait="Mackerel Strip" condition="not HasAtLeast(39881,&Basilosaurus_Rex;) and HasAtLeast(36593,1) and &TIME_WINDOW; and &WEATHER_PAST; and &WEATHER_NOW;"
				MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" CordialType="Auto" KeepNone="True" Sit="True" SitRate="1"
				Mooch="100" Patience="Patience2" MinimumGPPatience="650" UseMooch2="True">
			<FishSpots>
				
			</FishSpots>
			<Keepers>
				<Keeper name="Basilosaurus Rex" />
			</Keepers>
			<SurfaceSlaps />
		</ExFish>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks>
		<CodeChunk Name="AnglersArtToGP">
			<![CDATA[
				while (Core.Player.Auras.GetAuraStacksById(2778) >= 3)
				{
					await Coroutine.Sleep(500);
					ff14bot.Managers.ActionManager.DoAction(26804, Core.Player);
					await Coroutine.Sleep(1000);
				}
			]]>
		</CodeChunk>
	</CodeChunks>
</Profile>