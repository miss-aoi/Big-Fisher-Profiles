<!DOCTYPE Profile [
	<!-- Adjust to desired amount -->
	<!ENTITY Browned_Banana_Eel "1">
]>
<Profile>
	<Name>Browned Banana Eel</Name>
	<BehaviorDirectory />
	<Order>
		<Log Message="Starting Fishing: Browned Banana Eel..." />
		<If condition="not HasAtLeast(36591,50)">
			<!--Mayfly-->
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':36591,'Amount':50,'Enabled':true,'Type':'Exchange'}]" />
		</If>
		<If condition="not IsOnSubMap(3895)">
			<LisbethTravel Area="Thavnair (West)" XYZ="101.6033, 4.149, -347.1885" />
		</If>
		<If condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While condition="IsTimeBetween(21,22)">
			<WaitTimer WaitTime="30" />
		</While>
		<While condition="not HasAtLeast(38833,&Browned_Banana_Eel;) and HasAtLeast(36591,1) and (IsTimeBetween(22,24) or IsTimeBetween(0,2)) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(957, 'Rain')">
			<ExFish Bait="Mayfly" condition="not HasAtLeast(38833,&Browned_Banana_Eel;) and HasAtLeast(36591,1) and (IsTimeBetween(22,24) or IsTimeBetween(0,2)) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(957, 'Rain')" MinFish="15" MaxFish="25" ShuffleFishSpots="True" ThaliaksFavor="True" Cordial="Auto" Sit="True" Chum="True">
				<FishSpots>
					<FishSpot XYZ="101.6033, 4.149, -347.1885" Heading="1.043284" />
					<FishSpot XYZ="125.57, 4.149, -327.9035" Heading="4.310533" />
					<FishSpot XYZ="140.0963, 4.149, -359.2698" Heading="4.321018" />
				</FishSpots>
				<Keepers>
					<Keeper name="Browned Banana Eel" />
				</Keepers>
				<SurfaceSlaps />
			</ExFish>
		</While>
		<LLoadProfile Path="../../Start.xml" />
	</Order>
	<GrindAreas />
	<CodeChunks />
</Profile>